{
  "name": "enhanced-zorro-table",
  "version": "0.0.9",
  "scripts": {
    "start": "ng serve enhanced-zorro-table-demo --port=4231 --open",
    "test": "ng test enhanced-zorro-table",
    "ci:test": "ng test enhanced-zorro-table --browsers=ChromeHeadless --watch=false --code-coverage",
    "lint": "ng lint",
    "build-lib": "ng-packagr -p projects/enhanced-zorro-table/ng-package.json",
    "build-demo": "ng build enhanced-zorro-table-demo --prod --base-href=https://erikyu.github.io/enhanced-zorro-table/ --output-path=docs",
    "prettier": "yarn run prettier-demo && yarn run prettier-lib",
    "prettier-lib": "prettier --write \"projects/enhanced-zorro-table/**/*.*\"",
    "prettier-demo": "prettier --write \"projects/enhanced-zorro-table-demo/src/app/**/*.*\"",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "ng-zorro-antd": "^11.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  }
}
